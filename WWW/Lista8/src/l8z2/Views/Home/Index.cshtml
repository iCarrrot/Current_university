@model IEnumerable<l8z2.Models.Person>
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
</head>
<body>

    <table>
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.FirstName</td>
                <td>@p.LastName</td>
                <td>@p.Address</td>
                <td><a href="@Url.Action("Details", new { id = p.Id })">Szczegóły</a></td>
            </tr>
        }
    </table>
    @foreach (var link in ViewBag.Links)
    {
        <!--
        <a asp-controller="Home"
           asp-action="Index"
           asp-route-id="@link[0]">
           @link[1]
        </a>
        -->
        <a href="@Url.Action("Index", new { id = link[0] })">
            @link[1]
        </a>
    }

</body>
</html>